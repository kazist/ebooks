    "query": {
        "from": {
            "table": "Ebooks\\Ebooks\\Freebooks\\Code\\Tables\\Freebooks",
            "alias": "eef",
            "name": "#__ebooks_ebooks_freebooks"
        },
        "select": [
            "eef.id",
            "eef.ebook_id",
            "ee.title AS ebook_id_title",
            "eef.limit_id",
            "el.group_id AS limit_id_group_id",
            "el.book_limit AS limit_id_book_limit",
            "eef.start_date",
            "eef.end_date",
            "eef.created_by",
            "eef.date_created",
            "eef.modified_by",
            "eef.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Ebooks\\Ebooks\\Code\\Tables\\Ebooks",
                "alias": "ee",
                "name": "#__ebooks_ebooks",
                "on": "eef.ebook_id = ee.id"
            },
            {
                "table": "Ebooks\\Limits\\Code\\Tables\\Limits",
                "alias": "el",
                "name": "#__ebooks_limits",
                "on": "eef.limit_id = el.id"
            }
        ]
    }
