    "query": {
        "from": {
            "table": "Ebooks\\Ebooks\\Code\\Tables\\Ebooks",
            "alias": "ee",
            "name": "#__ebooks_ebooks"
        },
        "select": [
            "ee.id",
            "ee.category_id",
            "ec.title AS category_id_title",
            "ee.title",
            "ee.description",
            "ee.file",
            "mm.title AS file_title",
            "mm.file AS file_file",
            "ee.image",
            "mm_5094.title AS image_title",
            "mm_5094.file AS image_file",
            "ee.is_free",
            "ee.published",
            "ee.created_by",
            "ee.date_created",
            "ee.modified_by",
            "ee.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Ebooks\\Categories\\Code\\Tables\\Categories",
                "alias": "ec",
                "name": "#__ebooks_categories",
                "on": "ee.category_id = ec.id"
            },
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm",
                "name": "#__media_media",
                "on": "ee.file = mm.id"
            },
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm_5094",
                "name": "#__media_media",
                "on": "ee.image = mm_5094.id"
            }
        ]
    }
