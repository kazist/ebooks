    "right_field": "",
    "query": {
        "from": {
            "table": "Ebooks\\Ebooks\\Users\\Code\\Tables\\Users",
            "alias": "eeu",
            "name": "#__ebooks_ebooks_users"
        },
        "select": [
            "eeu.id",
            "eeu.limit_id",
            "el.frequency AS limit_id_frequency",
            "el.book_limit AS limit_id_book_limit",
            "eeu.ebook_id",
            "ee.title AS ebook_id_title",
            "eeu.user_id",
            "uu.name AS user_id_name",
            "eeu.frequency",
            "eeu.is_active",
            "eeu.created_by",
            "eeu.date_created",
            "eeu.modified_by",
            "eeu.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Ebooks\\Limits\\Code\\Tables\\Limits",
                "alias": "el",
                "name": "#__ebooks_limits",
                "on": "eeu.limit_id = el.id"
            },
            {
                "table": "Ebooks\\Ebooks\\Code\\Tables\\Ebooks",
                "alias": "ee",
                "name": "#__ebooks_ebooks",
                "on": "eeu.ebook_id = ee.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "eeu.user_id = uu.id"
            }
        ]
    }
