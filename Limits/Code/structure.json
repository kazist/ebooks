    "query": {
        "from": {
            "table": "Ebooks\\Limits\\Code\\Tables\\Limits",
            "alias": "el",
            "name": "#__ebooks_limits"
        },
        "select": [
            "el.id",
            "el.group_id",
            "ug.name AS group_id_name",
            "el.book_limit",
